<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "cr" table = "cr">
      
      
      
      <id name = "id" type = "int" column = "ComplaintID">
         <generator class="native"/>
      </id>
      
      <property name = "ComplaintType" column = "ComplaintType" type = "string"/>
      <property name = "CDescription" column = "CDescription" type = "string"/>
      <property name = "Severity" column = "Severity" type = "int"/>
      
   </class>
</hibernate-mapping>